shader TestShader : ComputeColor, Texturing, PVTest, CellLitghing
{
    override float4 Compute()
    {
        float4 color = CellLightTex.Sample(LinearSampler, streams.TexCoord);
        color.rgb = color.rgb / (1 + color.rgb);
        return color;


    }
};

